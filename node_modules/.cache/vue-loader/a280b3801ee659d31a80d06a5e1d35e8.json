.setSearch(value)}\n            }\n        },\n\n        methods: {\n            ...mapActions('ccenter/agents', {\n                addParentItem: 'ADD_ITEM',\n            }),\n\n            ...mapActions('ccenter/agents/queues', {\n                setParentId: 'SET_PARENT_ITEM_ID',\n                loadDataList: 'LOAD_DATA_LIST',\n                setSize: 'SET_SIZE',\n                setSearch: 'SET_SEARCH',\n                nextPage: 'NEXT_PAGE',\n                prevPage: 'PREV_PAGE',\n            }),\n        }\n    }\n</script>\n\n<style lang=\"scss\" scoped>\n    .inline-dropdown {\n        width: 226px;\n    }\n</style>"]}]}