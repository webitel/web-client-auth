ld.csv\"\n                                :placeholder=\"field.name\"\n                                :options=\"computeHeaders\"\n                                hide-label\n                                hide-details\n                        ></dropdown-select>\n                    </li>\n                </ul>\n\n            </div>\n        </section>\n    </popup>\n</template>\n\n<script>\n    import uploadCSVMixin from '../../../mixins/uploadCSVMixin';\n    import {addUser} from \"../../../api/directory/users/users\";\n\n    export default {\n        name: \"upload-users-popup\",\n        mixins: [uploadCSVMixin],\n        data() {\n            return {\n                mappingFields: [\n                    {\n                        name: 'username',\n                        required: true,\n                        csv: {}\n                    },\n                    {\n                        name: 'name',\n                        required: false,\n                        csv: {}\n                    },\n                    {\n                        name: 'extension',\n                        required: false,\n                        csv: {}\n                    },\n                    {\n                        name: 'email',\n                        required: false,\n                        csv: {}\n                    },\n                ],\n            }\n        },\n\n        methods: {\n            async addItem(item) {\n                await addUser(item);\n            }\n        }\n    }\n</script>\n\n<style lang=\"scss\" scoped>\n    @import \"../../../assets/css/objects/upload-popup\";\n</style>"]}]}