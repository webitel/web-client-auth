\n            noGutters: {\n                type: Boolean,\n                default: false\n            },\n\n            primaryText: {\n                type: String,\n                default: 'Add new',\n            },\n\n            primaryAction: {\n                type: Function\n            },\n\n            primaryDisabled: {\n                type: Boolean,\n                default: false,\n            },\n        },\n        methods: {\n            close() {\n                this.$emit('close');\n            }\n        }\n    }\n</script>\n\n<style lang=\"scss\">\n    .popup-wrap {\n        position: fixed;\n        top: 0;\n        right: 0;\n        bottom: 0;\n        left: 0;\n        z-index: 900;\n    }\n\n    .popup-bg {\n        position: absolute;\n        top: 0;\n        right: 0;\n        bottom: 0;\n        left: 0;\n        background: rgba(0, 0, 0, 0.25);\n        z-index: 0;\n    }\n\n    .popup {\n        @extend .scrollbar;\n        @extend .box-shadow;\n\n        position: absolute;\n        top: 50%;\n        left: 50%;\n        width: 738px;\n        max-height: 80vh;\n        padding: 27px 0;\n        background: #fff;\n        transform: translate(-50%, -50%);\n        border-radius: $border-radius;\n        overflow-y: auto;\n        z-index: 10;\n\n        &.overflow {\n            overflow: visible;\n        }\n\n        .content-header {\n            margin: 0 44px 28px;\n        }\n\n        .content-body {\n            padding: 0 44px;\n\n            &.no-gutters {\n                padding: 0;\n            }\n        }\n\n        .btn-controls {\n            text-align: right;\n            margin: 26px 44px 0;\n        }\n    }\n</style>"],"sourceRoot":"src/components/utils"}]}