patchItem({prop: 'r', index: props.rowIndex})\"\n                ></checkbox>\n            </template>\n\n            <template slot=\"edit\" slot-scope=\"props\">\n                <checkbox\n                        :value=\"dataList[props.rowIndex].access.u\"\n                        :label=\"$t('objects.allow')\"\n                        @input=\"patchItem({prop: 'u', index: props.rowIndex})\"\n                ></checkbox>\n            </template>\n\n            <template slot=\"delete\" slot-scope=\"props\">\n                <checkbox\n                        :value=\"dataList[props.rowIndex].access.d\"\n                        :label=\"$t('objects.allow')\"\n                        @input=\"patchItem({prop: 'd', index: props.rowIndex})\"\n                ></checkbox>\n            </template>\n        </vuetable>\n        <pagination\n                v-show=\"isLoaded\"\n                v-model=\"size\"\n                @loadDataList=\"loadList\"\n                @next=\"nextPage\"\n                @prev=\"prevPage\"\n                :isNext=\"isNextPage\"\n                :isPrev=\"!!page\"\n        ></pagination>\n    </section>\n</template>\n\n<script>\n    import rolePopup from './opened-resource-group-permissions-role-popup';\n    import tableComponentMixin from '@/mixins/tableComponentMixin';\n    import editComponentMixin from '@/mixins/editComponentMixin';\n    import {mapActions, mapState} from \"vuex\";\n\n    export default {\n        name: \"opened-resource-group-permissions\",\n        mixins: [tableComponentMixin, editComponentMixin],\n        components: {rolePopup},\n        data() {\n            return {\n                // vuetable prop\n                fields: [\n                    {name: 'grantee', title: this.$t('objects.name')},\n                    {name: 'read', title: this.$t('objects.read')},\n                    {name: 'edit', title: this.$t('objects.edit')},\n                    {name: 'delete', title: this.$t('objects.delete')},\n                ],\n            };\n        },\n\n        watch: {\n            parentId: function (value) {\n                this.setParentId(value);\n            }\n        },\n\n        computed: {\n            ...mapState('ccenter/resGroups', {\n                parentId: state => state.itemId,\n            }),\n            ...mapState('ccenter/resGroups/permissions', {\n                dataList: state => state.dataList,\n                page: state => state.page,\n                isNextPage: state => state.isNextPage,\n            }),\n\n            size: {\n                get() {\n                    return this.$store.state.ccenter.resGroups.permissions.size\n                },\n                set(value) {\n                    this.setSize(value)\n                }\n            },\n\n            search: {\n                get() {\n                    return this.$store.state.ccenter.resGroups.permissions.search\n                },\n                set(value) {\n                    this.setSearch(value)\n                }\n            }\n        },\n\n        methods: {\n            ...mapActions('ccenter/resGroups/permissions', {\n                setParentId: 'SET_PARENT_ITEM_ID',\n                loadDataList: 'LOAD_DATA_LIST',\n                setSize: 'SET_SIZE',\n                setSearch: 'SET_SEARCH',\n                nextPage: 'NEXT_PAGE',\n                prevPage: 'PREV_PAGE',\n                patchItem: 'PATCH_ITEM_PERMISSIONS',\n            }),\n        },\n    }\n</script>\n\n<style lang=\"scss\" scoped>\n\n</style>"],"sourceRoot":"src/components/contact-center/resource-groups"}]}